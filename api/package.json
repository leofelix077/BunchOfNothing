    "lint:tsc": "tsc",
    "lint:prettier": "prettier-check 'src/**/*.{js,ts}'",
    "lint-staged:prettier": "prettier-check",
    "lint:eslint": "eslint 'src/**/*.{js,ts}'",
    "lint-staged:eslint": "eslint",
    "build": "babel src -d build --source-maps --extensions '.ts,.js' --ignore '**/*.test.js,**/*.test.ts'"
  },
  "author": "",
